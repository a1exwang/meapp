{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.4",
  "body": [
    {
      "type": "FactSet",
      "facts": [
        {
          "title": "Name",
          "value": "${name}"
        },
        {
          "title": "Age",
          "value": "${age}"
        },
        {
          "title": "Gender",
          "value": "${gender}"
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "Action.OpenUrl",
      "title": "Open URL",
      "url": "https://www.google.com"
    },
    {
      "type": "Action.Submit",
      "title": "Submit",
      "data": {
        "action": "submitMain"
      }
    }

  ]
}